{
  "expo": {
    "name": "ToolLog",
    "slug": "tool-log",
    "version": "1.0.0",
    "scheme": "tool-log",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-font",
      "react-native-nfc-manager"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cyborg.toollog"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cyborg.toollog"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fb6429c8-94ad-460c-8df0-31a6a78fbe11"
      }
    },
    "owner": "teamcyborg"
  }
}
